generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum priorityEnum {
    low
    medium
    high
}

model Item {
    id        String    @id @default(uuid())
    text     String
    completed    Boolean @default(false)   
    dueDate DateTime?
    priority    priorityEnum?

    create_at DateTime  @default(now())
}
