generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum Status {
    Pending
    Completed
}

model item {
    id        String    @id @default(uuid())
    title     String
    detail    String?
    status    Status @default(Pending)   
    scheduled DateTime?
    
    create_at DateTime  @default(now())
}
